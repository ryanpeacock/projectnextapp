{
  "projects": {
    "default": {
      "buildCommand": "next build && build-storybook -o storybook-static"
    },
    "nextjs": {
      "buildCommand": "next build",
      "outputDirectory": ".next"
    },
    "storybook": {
      "buildCommand": "build-storybook -o storybook-static",
      "outputDirectory": "storybook-static"
    }
  }
}
